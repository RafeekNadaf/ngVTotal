.loc_date{font-size:12px;color:#898989;margin-bottom:5px;}
.b_cost{color:#1B8401;display:inline-block;border:1px #1B8401 dotted;font-size:15px;padding:5px;}
.booking_listitem .col{margin-bottom:10px;word-wrap:break-word;}
.booking_listitem .text-right{text-align:left;}
.bd_print_btn{margin-bottom:20px;}
.bk_id{padding-bottom:15px;}
.bookingDetailpage{padding-bottom:20px !important;}
.mrgn_mob{margin-bottom:10px;}
.booking_details .mrgn_mob{padding-left:0px; padding-right:0;}
.day_itemwrap .col-sm-2.text-right{text-align:left;}
.price_row{margin:0 -5px;border-bottom:1px #eee solid;background:#fff;}
.price_row.rh{padding:8px 5px 5px 5px;}
.price_row .col-xs-6{padding:0;}
.price_ico{display:inline-block;width:58px;height:22px;background-image:url(../images/combi.png); background-repeat:no-repeat;background-position:-178px -104px;}
.group_ico{display:inline-block;width:58px;height:22px;background-image:url(../images/combi.png); background-repeat:no-repeat;background-position:-178px -128px;}
.group_text{padding:3px 15px;font-size:14px;color:#212f50;font-weight:600;}
.price_text{padding:3px 0px 3px 5px;font-size:15px;color:#90dfaa;line-height:1;}
.alert-messages{position:fixed;background:#EB1A04;border-radius:10px;color:#fff;left:0;right:0;color:#fff;font-size:13px;max-width:280px;margin:0 auto;top:-100%;transition:top 0.5s ease 0.5s;text-align:left;z-index:10000000;padding:10px;box-shadow:0px 0px 3px #aaa;}
.alert-messages.slide-down{top:10px;}
.alert{padding:8px;}
@media (min-width:491px){
