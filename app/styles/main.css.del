.messages .message{clear:both;overflow:hidden;margin-bottom:20px;transition:all .5s linear;opacity:0;list-style:none;}.messages .message.left .avatar{background-color:#f5886e;float:left}.messages .message.left .text_wrapper{background-color:#f7f7f7;margin-left:11px}.messages .message.left .text_wrapper::after,.messages .message.left .text_wrapper::before{right:100%;border-right-color:#f7f7f7}.messages .message.left .text{color:#454545}.messages .message.right .avatar{background-color:#fdbf68;float:right}.messages .message.right .text_wrapper{background-color:#f0f0f0;margin-right:11px;float:right}.messages .message.right .text_wrapper::after,.messages .message.right .text_wrapper::before{left:100%;border-left-color:#f0f0f0}.messages .message.right .text{color:#454545;}.messages .message.appeared{opacity:1}.messages .message .avatar{width:40px;height:40px;border-radius:50%;display:inline-block}.messages .message .text_wrapper{display:inline-block;padding:10px;border-radius:6px;width:calc(100% - 85px);min-width:100px;position:relative}.messages .message .text_wrapper::after,.messages .message .text_wrapper:before{top:18px;border:solid transparent;content:" ";height:0;width:0;position:absolute;pointer-events:none}.messages .message .text_wrapper::after{border-width:6px;margin-top:0}.messages .message .text_wrapper::before{border-width:10px;margin-top:-5px}.messages .message .text_wrapper .text{font-size:14px;font-weight:300}
